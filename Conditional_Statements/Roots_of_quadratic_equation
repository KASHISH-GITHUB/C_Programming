#include<stdio.h>
#include<math.h>
int main()
{
    int d,r1,r2;
    int b,a,c;
    printf("enter the value of a,b,c respectively");
    scanf("%d%d%d",&a,&b,&c);
    d=b*b-4*a*a*c;
    if(d>0)
    {
        r1 = -b+sqrt (d) / (2*a);
        r2 = -b-sqrt (d) / (2*a);
        printf("the real roots are %d and %d ",r1,r2);  
    }
    else if(d==0)
    {
        r1=-b/(2*a);
        r2=-b/(2*a);
        printf("the roots are equal i.e %d and %d",r1,r2);
    }
    else
    {
        printf("Roots are imaginary");
    }

}
