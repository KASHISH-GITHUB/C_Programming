#include<stdio.h>
int main()
{
    int i,n,f=0,s=1,t;
    printf("enter a number upto which you want to print fibbonaci series: ");
    scanf("%d",&n);
    printf("%d",f);
    printf("%d",s);
    for(i=2;i<=n;i++)
    {
    t=f+s;
    printf("%d",t);
        f=s;
        s=t;
        t=f+s;   
    }
return 0;
}
