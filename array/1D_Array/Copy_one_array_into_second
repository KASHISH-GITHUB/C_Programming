#include<stdio.h>
int main()
{
    int i;
    int n;
    printf("enter the size of array");
    scanf("%d",&n);
    int arr[n];
    int brr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",arr[i]);
    }
    for(i=0;i<n;i++)
    {
        brr[i]=arr[i];
    }
    for(i=0;i<n;i++)
    {
        printf("%d",brr[i]);
    }
    
}
