#include<stdio.h>
int main()
{
    int n,i,max;
    printf("enter the size of an array :");
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
     max=arr[0];
    for(i=0;i<n;i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
        }
        
    }
    printf(" \nmaximum element of the array are : \n%d",max);
    return 0;
}
