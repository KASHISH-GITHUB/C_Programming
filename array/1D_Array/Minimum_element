#include<stdio.h>
int main()
{
    int n,i,min;
    printf("enter the size of an array :");
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
     min=arr[0];
    for(i=0;i<n;i++)
    {
        if(min>arr[i])
        {
            min=arr[i];
        }
        
    }
    printf(" \nminimum element of the array are : \n%d",min);
    return 0;
}
