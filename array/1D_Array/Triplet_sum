#include<stdio.h>
int main()
{
    int i,j,k,x=18,n,triplepair=0;
    printf("enter the size of an array :");
    scanf("%d",n);
    int arr[n];
    printf("enter the elements of array");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            for(k=j+2;k<n;k++)
            {
                if(arr[i]+arr[j]+arr[k]==x)
                {
                    triplepair++;
                }
            }
        printf("%d",triplepair);
        }
    }
}
