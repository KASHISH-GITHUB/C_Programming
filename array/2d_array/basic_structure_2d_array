#include<stdio.h>
int main()
{
    int i,j,r,c;
    printf("enter the number of rows in array");
    scanf("%d",&r);
    printf("enter the number of columns in array");
    scanf("%d",&c);
    int arr[r][c];
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            printf("%d",arr[i][j]);
        }
         printf("\n");
    }

}
