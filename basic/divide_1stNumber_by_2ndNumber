#include<stdio.h>
int main()
{
    float a,b,c;
    printf("enter first number : ");
    scanf("%f",&a);
    printf("enter second number : ");
    scanf("%f",&b);
    c=a/b;
    printf("the division of first number by second number is %f",c);
    printf("\n i.e. %f / %f =%f",a,b,c);
    return 0;
    

}
