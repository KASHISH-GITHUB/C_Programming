#include<stdio.h>
int main()
{
    int m1,m2,m3,m4,m5,totalmarks;
    float avgmarks;
    printf("enter the marks of sst\n");
    scanf("%d",&m1);
    printf("enter the marks of english\n");
    scanf("%d",&m2);
    printf("enter the marks of hindi\n");
    scanf("%d",&m3);
    printf("enter the marks of gk\n");
    scanf("%d",&m4);
    printf("enter the marks of evs\n");
    scanf("%d",&m5);
    totalmarks=m1+m2+m3+m4+m5;
    printf("\nthe total marks of all these subjects are %d\n",totalmarks);
    avgmarks=totalmarks/5.0;
     printf("the average marks of all these subjects are %f",avgmarks);
    }
